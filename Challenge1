list_numbers = [1,2,3,5,7,8,11,12,15,17,19,21,25,29]
#print(sum(list_numbers))
d=2
even_numbers = []
while d<=2:
    for number in list_numbers:
        if number % d==0:
            even_numbers.append(number)
            number=number/d
        else:
            
print("My even numbers are:",even_numbers)
        
            


